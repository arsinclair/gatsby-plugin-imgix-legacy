{
  "name": "gatsby-plugin-imgix-legacy",
  "version": "1.0.0",
  "description": "Fork of gatsby-plugin-imgix to make it work with most recent versions of GatsbyJS",
  "license": "BSD-2-Clause",
  "main": "dist/gatsby-plugin-imgix-legacy.js",
  "module": "dist/gatsby-plugin-imgix-legacy.modern.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "fragments.js"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "yarn clean && microbundle watch src/index.ts src/node.ts src/gatsby-node.ts -o dist -f modern,cjs",
    "build": "yarn clean && microbundle build src/index.ts src/node.ts src/gatsby-node.ts -o dist -f modern,cjs",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "yarn build"
  },
  "author": "Angelo Ashmore",
  "contributors": [
    "Raman Sinclair"
  ],
  "homepage": "https://github.com/arsinclair/gatsby-plugin-imgix-legacy",
  "repository": {
    "type": "git",
    "url": "https://github.com/arsinclair/gatsby-plugin-imgix-legacy"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "imgix",
    "images"
  ],
  "dependencies": {
    "camel-case": "^4.1.2",
    "fp-ts": "^2.9.5",
    "imgix-url-params": "^11.11.2",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "param-case": "^3.0.4"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.21",
    "@types/md5": "^2.3.0",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "gatsby": "^3.1.1",
    "gatsby-image": "^3.1.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
